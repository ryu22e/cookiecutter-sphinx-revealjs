{
  "project_name": "cookiecutter sphinx revealjs",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '-')|replace('_', '-')|replace('.', '-')|trim() }}",
  "title": "Example",
  "python_version": [
      "3.13",
      "3.12",
      "3.11",
      "3.10"
  ],
  "site_url": "https://example.com",
  "copyright_year": "2026",
  "author_name": "Ryuji Tsutsui",
  "release": "1.0",
  "language": "ja",
  "enable_budoux": "y",
  "enable_robots_txt": "y",
  "enable_favicon": "y",
  "disable_title_uppercase": "n",
  "enable_embed_video": "n",
  "style": [
      "reStructuredText",
      "Markdown"
  ],
  "revealjs_style_theme": [
    "black",
    "white",
    "league",
    "beige",
    "sky",
    "night",
    "serif",
    "simple",
    "solarized",
    "blood",
    "moon"
  ],
  "license": [
      "CC BY 4.0",
      "None"
  ],
  "_extensions": ["local_extensions.rest.rest_title0"],
  "_copy_without_render": [
    ".github/workflows/static.yml"
  ]
}
