{
  "project_name": "cookiecutter sphinx revealjs",
  "project_slug": "{{ cookiecutter.project_name.lower()|replace(' ', '-')|replace('_', '-')|replace('.', '-')|trim() }}",
  "title": "Example",
  "copyright_year": "2023",
  "author_name": "Ryuji Tsutsui",
  "release": "1.0",
  "language": "ja",
  "enable_budoux": "y",
  "enable_robots_txt": "y",
  "enable_favicon": "y",
  "disable_title_uppercase": "n",
  "revealjs_style_theme": [
    "black",
    "white",
    "league",
    "beige",
    "sky",
    "night",
    "serif",
    "simple",
    "solarized",
    "blood",
    "moon"
  ],
  "license": [
      "CC BY 4.0",
      "None"
  ],
  "_extensions": ["local_extensions.rest.rest_title0"],
  "_copy_without_render": [
    ".github/workflows/static.yml"
  ]
}
